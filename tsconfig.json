{
  "compilerOptions": {
    "moduleResolution": "Node",
    "esModuleInterop": true,
    "baseUrl": "src",
    "outDir": "dist",
    "lib": [
      "ESNext",
      "DOM"
    ],
    "outDir": "./dist",
    "rootDir": "./src",
    "removeComments": true,
    "noEmitOnError": true,
    "strict": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
  },
  "include": [
    "./src"
  ]
}
